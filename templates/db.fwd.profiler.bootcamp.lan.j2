;
; BIND data file for local loopback interface
;

$TTL    604800
@       IN      SOA     profiler.bootcamp.lan. root.profiler.bootcamp.lan. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
; name servers - NS records
     IN      NS      profiler.bootcamp.lan.

; name servers - A records
profiler.bootcamp.lan.          IN      A       192.168.254.128
